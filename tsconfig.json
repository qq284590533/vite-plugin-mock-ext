{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "declaration": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "module": "commonjs",
    "lib": ["ESNext", "dom"],
    "types": ["vite/client","node"],
    "sourceMap": false,
    "noEmitOnError": true
  },
  "include": ["./src"],
  "exclude": ["**/dist"]
}
